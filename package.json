{
  "name": "backend_testing_ts",
  "version": "0.0.1",
  "description": "Template for express in typescript with babel and testing using jest and supertest",
  "main": "build/server.js",
  "scripts": {
    "test": "jest --runInBand",
    "build": "babel src --extensions \".ts\" --out-dir build",
    "pretest": "npm run build",
    "preserve": "npm run build",
    "serve": "node build/server.js"
  },
  "keywords": [
    "express",
    "ts",
    "jest",
    "supertest"
  ],
  "author": "droidmakk",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-transform-typescript": "7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "4.16.1",
    "@types/jest": "^24.0.11",
    "@types/mocha": "^5.2.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-minify": "^0.5.0",
    "jest": "24.7.1",
    "superagent": "5.0.2",
    "supertest": "4.0.2"
  },
  "dependencies": {
    "express": "4.17.3"
  }
}
